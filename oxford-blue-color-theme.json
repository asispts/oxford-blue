{
    "name": "oxford-blue",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "activityBar.background": "#001a39",
        "activityBar.foreground": "#efff73",
        "activityBar.inactiveForeground": "#0069e6",
        "activityBarBadge.background": "#efff73",
        "activityBarBadge.foreground": "#4B4E29",
        "breadcrumb.background": "#001a39",
        "dropdown.background": "#002959",
        "editor.background": "#001a39",
        "editor.foreground": "#72ABD6",
        "editor.lineHighlightBackground": "#002959",
        "editor.lineHighlightBorder": "#001a39",
        "editor.selectionBackground": "#0069e640",
        "editorCursor.foreground": "#efff73",
        "editorGroupHeader.tabsBackground": "#001a39",
        "editorLineNumber.activeForeground": "#efff73",
        "editorLineNumber.foreground": "#0069e6",
        "editorSuggestWidget.background": "#002147",
        "editorSuggestWidget.border": "#0069e6",
        "editorSuggestWidget.focusHighlightForeground": "#efff73",
        "editorSuggestWidget.highlightForeground": "#efff73",
        "editorSuggestWidget.selectedBackground": "#0069e640",
        "editorWidget.background": "#002147",
        "editorWidget.border": "#0069e6",
        "foreground": "#72ABD6",
        "input.background": "#001a39",
        "input.border": "#0069e6",
        "list.activeSelectionBackground": "#002959",
        "list.focusOutline": "#0069e6",
        "list.hoverBackground": "#002959",
        "list.inactiveSelectionBackground": "#002959",
        "panel.border": "#efff73",
        "sideBar.background": "#002147",
        "sideBarSectionHeader.background": "#001a39",
        "sideBarSectionHeader.foreground": "#2D658D",
        "sideBarTitle.foreground": "#2D658D",
        "statusBar.background": "#efff73",
        "statusBar.debuggingBackground": "#E6427C",
        "statusBar.debuggingForeground": "#001a39",
        "statusBar.foreground": "#4B4E29",
        "statusBar.noFolderBackground": "#0069e6",
        "statusBar.noFolderForeground": "#e1e4f8",
        "statusBarItem.hoverBackground": "#bdcf44",
        "tab.activeBackground": "#0069e640",
        "tab.activeForeground": "#e1e4f8",
        "tab.activeModifiedBorder": "#efff73",
        "tab.border": "#0069e6",
        "tab.hoverBackground": "#0069e640",
        "tab.hoverForeground": "#e1e4f8",
        "tab.inactiveBackground": "#001a39",
        "tab.inactiveForeground": "#2D658D",
        "tab.inactiveModifiedBorder": "#efff73"
    },
    "tokenColors": [
        {
            "name": "Default text",
            "scope": [
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#72ABD6"
            }
        },
        {
            "name": "Muted text",
            "scope": [
                "punctuation"
            ],
            "settings": {
                "foreground": "#2D658D"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#2D658D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#64de83"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#efff73"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant",
                "keyword.other.unit",
                "punctuation.definition.constant",
                "support.constant",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#8373ff"
            }
        },
        {
            "name": "Keyword Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#ffa65e",
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "keyword.operator.new",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#369CFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#369CFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Data type",
            "scope": [
                "keyword.other.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#ffa65e",
                "fontStyle": ""
            }
        },
        {
            "name": "Builtin classes and functions",
            "scope": [
                "support.function",
                "support.class.builtin"
            ],
            "settings": {
                "foreground": "#ff73ef"
            }
        },
        {
            "name": "Support class",
            "scope": [
                "support.class"
            ],
            "settings": {
                "foreground": "#ff7a73"
            }
        },
        {
            "name": "Function call",
            "scope": [
                "meta.function-call",
                "meta.method-call"
            ],
            "settings": {
                "foreground": "#9BC4E2"
            }
        },
        {
            "name": "Namespace path",
            "scope": [
                "entity.name.type.namespace",
                "support.other.namespace",
                "punctuation.separator.inheritance"
            ],
            "settings": {
                "foreground": "#2D658D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSON object key",
            "scope": [
                "support.type.property-name.json",
                "punctuation.support.type.property-name"
            ],
            "settings": {
                "foreground": "#ff7a73"
            }
        },
        {
            "name": "Makefile target",
            "scope": [
                "entity.name.function.target"
            ],
            "settings": {
                "foreground": "#13d3d3"
            }
        },
        {
            "name": "Makefile target",
            "scope": [
                "meta.scope.prerequisites"
            ],
            "settings": {
                "foreground": "#64de83"
            }
        },
        {
            "name": "Markdown headings",
            "scope": [
                "markup.heading.markdown",
                "punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#9BC4E2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown code block",
            "scope": [
                "punctuation.definition.markdown",
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "#2D658D"
            }
        },
        {
            "name": "Markdown block code",
            "scope": [
                "markup.fenced_code.block.markdown"
            ],
            "settings": {
                "foreground": "#176bcf"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": [
                "markup.inline.raw.string.markdown",
                "punctuation.definition.raw.markdown"
            ],
            "settings": {
                "foreground": "#ffa65e"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#13d3d3"
            }
        },
        {
            "name": "Markdown quotation",
            "scope": [
                "markup.quote.markdown"
            ],
            "settings": {
                "foreground": "#2D658D"
            }
        },
        {
            "name": "Markdown bold text",
            "scope": [
                "markup.bold.markdown",
                "punctuation.definition.bold.markdown"
            ],
            "settings": {
                "foreground": "#9BC4E2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic text",
            "scope": [
                "markup.italic.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#9BC4E2",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown strikethrough text",
            "scope": [
                "markup.strikethrough.markdown",
                "punctuation.definition.strikethrough.markdown"
            ],
            "settings": {
                "foreground": "#9BC4E2"
            }
        },
        {
            "name": "String",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#ff7a73"
            }
        },
        {
            "name": "Markup attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffa65e"
            }
        },
        {
            "name": "JSX component",
            "scope": [
                "support.class.component"
            ],
            "settings": {
                "foreground": "#13d3d3"
            }
        },
        {
            "name": "<?php and ?> symbols",
            "scope": [
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#ffa65e"
            }
        },
        {
            "name": "PHPDoc keywords",
            "scope": [
                "keyword.other.phpdoc"
            ],
            "settings": {
                "foreground": "#2D658D",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "PHP8 Attributes",
            "scope": [
                "support.attribute.php",
                "meta.attribute.php",
                "entity.name.variable.parameter"
            ],
            "settings": {
                "foreground": "#2D658D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "SCSS keyword",
            "scope": [
                "keyword.control.at-rule",
                "punctuation.definition.keyword",
                "punctuation.definition.interpolation"
            ],
            "settings": {
                "foreground": "#369CFF"
            }
        },
        {
            "name": "CSS class and ID selector",
            "scope": [
                "punctuation.definition.entity.css"
            ],
            "settings": {
                "foreground": "#ffa65e"
            }
        },
        {
            "name": "String template expression",
            "scope": [
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#ff7a73"
            }
        },
        {
            "name": "jsdoc keyword",
            "scope": [
                "punctuation.definition.block.tag.jsdoc",
                "storage.type.class.jsdoc"
            ],
            "settings": {
                "foreground": "#2D658D",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Typescript braces",
            "scope": [
                "meta.brace.round.ts",
                "meta.brace.square.ts"
            ],
            "settings": {
                "foreground": "#2D658D"
            }
        }
    ],
    "semanticTokenColors": {
        "*.defaultLibrary": "#ff7a73",
        "class.declaration": "#72ABD6",
        "function": "#E6427C",
        "function.declaration": "#72ABD6",
        "interface": "#ff7a73",
        "interface.declaration": "#72ABD6",
        "method": "#72ABD6",
        "method.declaration": "#E6427C",
        "method.defaultLibrary": "#ff73ef",
        "property": "#72ABD6",
        "property.declaration": "#13d3d3",
        "property.defaultLibrary": "#ff73ef",
        "type": "#ff7a73",
        "type.declaration": "#72ABD6"
    }
}