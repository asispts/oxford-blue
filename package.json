{
    "name": "oxford-blue",
    "displayName": "Oxford-blue Theme",
    "description": "A stylish VSCode dark theme for PHP, TypeScript, and frontend development",
    "publisher": "asispts",
    "license": "MIT",
    "version": "0.7.0",
    "icon": "icon.png",
    "keywords": [
        "dark-theme"
    ],
    "author": {
        "name": "Asis Pattisahusiwa",
        "email": "asispts@gmail.com",
        "url": "https://github.com/asispts"
    },
    "homepage": "https://github.com/asispts/oxford-blue",
    "repository": {
        "type": "git",
        "url": "https://github.com/asispts/oxford-blue"
    },
    "bugs": {
        "url": "https://github.com/asispts/oxford-blue/issues"
    },
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "oxford-blue",
                "uiTheme": "vs-dark",
                "path": "./oxford-blue-color-theme.json"
            }
        ]
    },
    "scripts": {
        "build": "ts-node src/main.ts",
        "dev": "ts-node-dev --respawn --files src/main.ts",
        "cs": "prettier -c src",
        "cs:fix": "prettier -w src",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix"
    },
    "devDependencies": {
        "@types/node": "^20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "eslint": "^8.41.0",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
}
