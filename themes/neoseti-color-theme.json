{
	"name": "neoseti",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		// +++++++++++++++++++++++++
		// Base colors
		// +++++++++++++++++++++++++
		"foreground": "#849FBE",
		// +++++++++++++++++++++++++
		// Activity Bar
		// +++++++++++++++++++++++++
		"activityBar.background": "#001a39",
		// +++++++++++++++++++++++++
		// Sidebar
		// +++++++++++++++++++++++++
		"sideBar.background": "#002147",
		"sideBarSectionHeader.background": "#001a39",
		// +++++++++++++++++++++++++
		// Editor
		// +++++++++++++++++++++++++
		"editor.background": "#072140",
		"editor.foreground": "#849FBE",
		"editor.lineHighlightBackground": "#002959",
		"editor.lineHighlightBorder": "#ff000000",
		"editorCursor.foreground": "#FF4081",
		"editorLineNumber.foreground": "#3D5066",
		"editorLineNumber.activeForeground": "#717E8C",
		"editor.selectionBackground": "#00336f",
		"editor.wordHighlightBackground": "#00336f",
		// +++++++++++++++++++++++++
		// Editor widget colors
		// +++++++++++++++++++++++++
		"editorSuggestWidget.background": "#090B0D",
		"editorSuggestWidget.foreground": "#293C44",
		"editorSuggestWidget.border": "#001833",
		"editorSuggestWidget.focusHighlightForeground": "#E39438",
		"editorSuggestWidget.highlightForeground": "#466674",
		"editorSuggestWidget.selectedBackground": "#13181c",
		"editorSuggestWidget.selectedForeground": "#7494A3",
		// +++++++++++++++++++++++++
		// Editor groups and tabs
		// +++++++++++++++++++++++++
		"editorGroupHeader.tabsBackground": "#002147",
		"tab.inactiveBackground": "#002147",
		"tab.activeBackground": "#001a39",
		"tab.border": "#001a39",
		// +++++++++++++++++++++++++
		// Breadcrumbs colors
		// +++++++++++++++++++++++++
		"breadcrumb.background": "#001a39",
		// +++++++++++++++++++++++++
		// Status bar
		// +++++++++++++++++++++++++
		"statusBar.background": "#001a39",
	},
	"tokenColors": [
		{
			"name": "Fallback text",
			"scope": [
				"entity",
				"meta",
				"text",
				"source"
			],
			"settings": {
				"foreground": "#FF3D00",
			}
		},
		// +++++++++++++++++++++++++
		// Comments
		// +++++++++++++++++++++++++
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0077FF",
			}
		},
		{
			"name": "Comment keyword",
			"scope": [
				"keyword.other.phpdoc",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0077FF",
			}
		},
		{
			"name": "Built-in functions",
			"scope": [
				"support.function",
			],
			"settings": {
				"foreground": "#FF8A65",
			}
		},
		{
			"name": "Punctuation, namespace, and muted text",
			"scope": [
				"punctuation",
				"support.other.namespace",
				"entity.name.type.namespace",
				"meta.class"
			],
			"settings": {
				"foreground": "#5A6570",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"punctuation.definition.variable",
			],
			"settings": {
				"foreground": "#ffe140",
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
			],
			"settings": {
				"foreground": "#B388FF",
			}
		},
		{
			"name": "Keywords and storage",
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type",
				"support.type.property-name",
				"punctuation.support.type.property-name"
			],
			"settings": {
				"foreground": "#00E676",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#FF80AB",
			}
		},
		{
			"name": "Properties and method call",
			"scope": [
				"variable.other.property",
				"meta.method-call",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#57a6a8",
			}
		},
		{
			"name": "Dependencies",
			"scope": [
				"support.class",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#4DD0E1",
			}
		}
	]
}
