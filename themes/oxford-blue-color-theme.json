{
	"name": "neoseti",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		// +++++++++++++++++++++++++
		// Base colors
		// +++++++++++++++++++++++++
		"foreground": "#9bc4e2",
		// +++++++++++++++++++++++++
		// Activity Bar
		// +++++++++++++++++++++++++
		"activityBar.background": "#001a39",
		"activityBar.foreground": "#efff73",
		"activityBar.inactiveForeground": "#004699",
		"activityBarBadge.background": "#efff73",
		"activityBarBadge.foreground": "#001a39",
		// +++++++++++++++++++++++++
		// Sidebar
		// +++++++++++++++++++++++++
		"sideBar.background": "#002147",
		"sideBarTitle.foreground": "#efff73",
		"sideBarSectionHeader.background": "#001a39",
		"sideBarSectionHeader.foreground": "#9bc4e2",
		"list.hoverBackground": "#0A3466",
		"list.inactiveSelectionBackground": "#0A3466",
		"list.focusOutline": "#007FD4",
		"list.activeSelectionBackground": "#0A3466",
		// +++++++++++++++++++++++++
		// Editor
		// +++++++++++++++++++++++++
		"editor.background": "#001a39",
		"editor.foreground": "#9bc4e2",
		"editorCursor.foreground": "#efff73",
		"editorLineNumber.foreground": "#004699",
		"editorLineNumber.activeForeground": "#efff73",
		"editor.lineHighlightBackground": "#092E59",
		"editor.lineHighlightBorder": "#00000000",
		"editor.selectionBackground": "#596280",
		// +++++++++++++++++++++++++
		// Editor widget colors
		// +++++++++++++++++++++++++
		"editorSuggestWidget.background": "#002147",
		"editorSuggestWidget.foreground": "#5483a3",
		"editorSuggestWidget.border": "#007FD4",
		"editorSuggestWidget.selectedBackground": "#094771",
		"editorSuggestWidget.selectedForeground": "#9bc4e2",
		"editorSuggestWidget.focusHighlightForeground": "#efff73",
		"editorSuggestWidget.highlightForeground": "#efff73",
		// +++++++++++++++++++++++++
		// Editor groups and tabs
		// +++++++++++++++++++++++++
		"editorGroupHeader.tabsBackground": "#001a39",
		"tab.inactiveBackground": "#001a39",
		"tab.activeBackground": "#002147",
		"tab.activeForeground": "#9bc4e2",
		"tab.activeModifiedBorder": "#176BCF",
		"tab.activeBorder": "#efff73",
		"tab.hoverBackground": "#002147",
		"tab.hoverForeground": "#9bc4e2",
		"tab.inactiveForeground": "#5483a3",
		"tab.inactiveModifiedBorder": "#B387D3",
		"tab.border": "#002147",
		// +++++++++++++++++++++++++
		// Breadcrumbs colors
		// +++++++++++++++++++++++++
		"breadcrumb.background": "#001a39",
		"breadcrumb.foreground": "#5483a3",
		// +++++++++++++++++++++++++
		// Status bar
		// +++++++++++++++++++++++++
		"statusBar.background": "#efff73",
		"statusBar.foreground": "#072140",
		"statusBarItem.hoverBackground": "#bdcf44",
		// +++++++++++++++++++++++++
		// Editor widget
		// +++++++++++++++++++++++++
		"editorWidget.background": "#002147",
		"editorWidget.foreground": "#5483a3",
		"editorWidget.border": "#007FD4",
		// +++++++++++++++++++++++++
		// Input background
		// +++++++++++++++++++++++++
		"input.background": "#002147",
		"input.border": "#004699",
		// +++++++++++++++++++++++++
		// Dropdown
		// +++++++++++++++++++++++++
		"dropdown.background": "#002147",
		"dropdown.foreground": "#5483a3",
		"dropdown.border": "#004699",
		// +++++++++++++++++++++++++
		// Panel
		// +++++++++++++++++++++++++
		"panel.border": "#efff73"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#176BCF",
			}
		},
		{
			"name": "Comment keyword",
			"scope": [
				"keyword.other.phpdoc",
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#176BCF",
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#5a829e",
			}
		},
		{
			"name": "Muted text",
			"scope": [
				"support.other.namespace",
				"punctuation.separator.inheritance",
				"entity.name.type.namespace",
			],
			"settings": {
				"foreground": "#5a829e",
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"punctuation.section.embedded", // <?php ?>
				"keyword.other.unit", // scss e.q: 100%
				"punctuation.definition.constant", // scss e.q: #f00
				"support.constant", // php e.q: DIRECTORY_SEPARATOR
			],
			"settings": {
				"foreground": "#8373ff",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#64DE83",
			}
		},
		{
			"name": "Reset colors",
			"scope": [
				"variable.other.property",
			],
			"settings": {
				"foreground": "#9BC4E2",
			}
		},
		// +++++++++++++++++++++++++
		//
		// +++++++++++++++++++++++++
		{
			"name": "keywords, storage, HTML tags",
			"scope": [
				"keyword",
				"keyword.operator.new", // js new operator
				"storage.modifier",
				"storage.type",
				"support.type.property-name.json",
				"punctuation.support.type.property-name",
				"entity.name.tag",
				"punctuation.definition.tag",
				"support.type.primitive",
				"punctuation.definition.keyword", // scss e.q: @import
				"punctuation.definition.interpolation", // scss e.q: #{...}
				"entity.name.function.target", // makefile target
			],
			"settings": {
				"foreground": "#ffa65e",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"punctuation.definition.variable",
				"entity.other.attribute-name",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#efff73",
			}
		},
		{
			"name": "Built-in functions",
			"scope": [
				"support.function",
			],
			"settings": {
				"foreground": "#ff73ef",
			}
		},
		{
			"name": "functions call",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#9BC4E2",
			}
		},
		// +++++++++++++++++++++++++
		// Markdown
		// +++++++++++++++++++++++++
		{
			"name": "Markdown headings",
			"scope": [
				"entity.name.section",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#ffa65e",
			}
		},
		{
			"name": "Markdown italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF80C3",
			}
		},
		{
			"name": "Markdown bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF80C3",
			}
		},
		{
			"name": "Markdown inline code",
			"scope": [
				"markup.inline.raw",
				"punctuation.definition.raw"
			],
			"settings": {
				"foreground": "#efff73",
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#0AE0EE",
			}
		},
	]
}
