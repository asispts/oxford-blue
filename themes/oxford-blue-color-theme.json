{
	"name": "neoseti",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		// +++++++++++++++++++++++++
		// Base colors
		// +++++++++++++++++++++++++
		"foreground": "#9bc4e2",
		// +++++++++++++++++++++++++
		// Activity Bar
		// +++++++++++++++++++++++++
		"activityBar.background": "#001a39",
		"activityBar.foreground": "#E4E413",
		"activityBar.inactiveForeground": "#004699",
		"activityBarBadge.background": "#E4E413",
		"activityBarBadge.foreground": "#001a39",
		// +++++++++++++++++++++++++
		// Sidebar
		// +++++++++++++++++++++++++
		"sideBar.background": "#002147",
		"sideBarTitle.foreground": "#E4E413",
		"sideBarSectionHeader.background": "#001a39",
		"sideBarSectionHeader.foreground": "#9bc4e2",
		"list.hoverBackground": "#0A3466",
		"list.inactiveSelectionBackground": "#0A3466",
		"list.focusOutline": "#007FD4",
		"list.activeSelectionBackground": "#0A3466",
		// +++++++++++++++++++++++++
		// Editor
		// +++++++++++++++++++++++++
		"editor.background": "#001a39",
		"editor.foreground": "#9bc4e2",
		"editorCursor.foreground": "#E4E413",
		"editorLineNumber.foreground": "#004699",
		"editorLineNumber.activeForeground": "#E4E413",
		"editor.lineHighlightBackground": "#092E59",
		"editor.lineHighlightBorder": "#00000000",
		"editor.selectionBackground": "#596280",
		// +++++++++++++++++++++++++
		// Editor widget colors
		// +++++++++++++++++++++++++
		"editorSuggestWidget.background": "#002147",
		"editorSuggestWidget.foreground": "#5483a3",
		"editorSuggestWidget.border": "#007FD4",
		"editorSuggestWidget.selectedBackground": "#094771",
		"editorSuggestWidget.selectedForeground": "#9bc4e2",
		"editorSuggestWidget.focusHighlightForeground": "#E4E413",
		"editorSuggestWidget.highlightForeground": "#E4E413",
		// +++++++++++++++++++++++++
		// Editor groups and tabs
		// +++++++++++++++++++++++++
		"editorGroupHeader.tabsBackground": "#002147",
		"tab.inactiveBackground": "#002147",
		"tab.activeBackground": "#0A3466",
		"tab.activeForeground": "#9bc4e2",
		"tab.activeModifiedBorder": "#176BCF",
		"tab.activeBorder": "#E4E413",
		"tab.hoverBackground": "#0A3466",
		"tab.hoverForeground": "#9bc4e2",
		"tab.inactiveForeground": "#5483a3",
		"tab.inactiveModifiedBorder": "#B387D3",
		"tab.border": "#002d61",
		// +++++++++++++++++++++++++
		// Breadcrumbs colors
		// +++++++++++++++++++++++++
		"breadcrumb.background": "#001a39",
		"breadcrumb.foreground": "#5483a3",
		// +++++++++++++++++++++++++
		// Status bar
		// +++++++++++++++++++++++++
		"statusBar.background": "#E4E413",
		"statusBar.foreground": "#072140",
		// +++++++++++++++++++++++++
		// Editor widget
		// +++++++++++++++++++++++++
		"editorWidget.background": "#002147",
		"editorWidget.foreground": "#5483a3",
		"editorWidget.border": "#007FD4",
		// +++++++++++++++++++++++++
		// Input background
		// +++++++++++++++++++++++++
		"input.background": "#002147",
		"input.border": "#004699",
		// +++++++++++++++++++++++++
		// Dropdown
		// +++++++++++++++++++++++++
		"dropdown.background": "#002147",
		"dropdown.foreground": "#5483a3",
		"dropdown.border": "#004699",
	},
	"tokenColors": [
		// +++++++++++++++++++++++++
		// Comments
		// +++++++++++++++++++++++++
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#176BCF",
			}
		},
		{
			"name": "Comment keyword",
			"scope": [
				"keyword.other.phpdoc",
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#176BCF",
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
			],
			"settings": {
				"foreground": "#B387D3",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#50d0c3",
			}
		},
		{
			"name": "keywords, storage, HTML tags",
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type",
				"support.type.property-name",
				"punctuation.support.type.property-name",
				"entity.name.tag",
				"punctuation.definition.tag",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#DD9719",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"punctuation.definition.variable",
				"entity.other.attribute-name",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#bdda57",
			}
		},
		{
			"name": "Dependencies",
			"scope": [
				"support.class",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#46d683",
			}
		},
		{
			"name": "Properties and method call",
			"scope": [
				"variable.other.property",
				"meta.method-call",
				"meta.function-call",
			],
			"settings": {
				"foreground": "#19b2e6",
			}
		},
		{
			"name": "Built-in functions",
			"scope": [
				"support.function",
			],
			"settings": {
				"foreground": "#FF8A65",
			}
		},
		{
			"name": "Punctuation, namespace, and muted text",
			"scope": [
				"punctuation",
				"support.other.namespace",
				"entity.name.type.namespace",
				"meta.class",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#5483a3",
			}
		},
	]
}
